	<subDataset name="summaryByVendor">
		<parameter name="DS_WARN_THRESH" class="java.lang.Integer" isForPrompting="false">
			<defaultValueExpression><![CDATA[30]]></defaultValueExpression>
		</parameter>
		<parameter name="DS_DISPLAY_EXPIRED" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA["true"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	count(*) as tally,
	a.manufacturer,
	a.maintcontract,
	CASE
		WHEN
			to_date(a.maintcontractexpires,'DD.MM.YYYY') - now() < INTERVAL '0 days'
 			THEN 'WARNING'
		ELSE
			'NORMAL'
	END AS maintcontractstatus
FROM
	public.assets a
JOIN
	public.node n
ON
	a.nodeid = n.nodeid
WHERE
	n.nodetype = 'A' AND
	a.maintcontract IS NOT NULL  AND
	a.maintcontractexpires IS NOT NULL  AND
	a.maintcontractexpires != '' AND
	a.maintcontractexpires != 'n.v.' AND
	a.maintcontract != 'n.v.' AND
	a.maintcontract != '' AND
	to_date(a.maintcontractexpires,'DD.MM.YYYY') - now() < INTERVAL '$P!{DS_WARN_THRESH} days'
GROUP BY
	a.manufacturer,
	a.maintcontract,
	maintcontractstatus
ORDER BY
	a.manufacturer ASC,
	a.maintcontract DESC]]>
		</queryString>
