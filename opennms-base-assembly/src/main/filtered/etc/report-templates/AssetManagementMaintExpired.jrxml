	<parameter name="WARNING_THRESHOLD_DAYS" class="java.lang.Integer">
		<parameterDescription><![CDATA[Warning threshold for maintenance contract expire.]]></parameterDescription>
		<defaultValueExpression><![CDATA[30]]></defaultValueExpression>
	</parameter>
	<parameter name="DISPLAY_EXPIRED" class="java.lang.String">
		<parameterDescription><![CDATA[Show mainenance expired reports. Allowed parameter input: true or false as string, default is true.]]></parameterDescription>
		<defaultValueExpression><![CDATA["true"]]></defaultValueExpression>
	</parameter>
	<parameter name="COMPANY_LOGO" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["${install.dir}/etc/report-templates/assets/images/company-logo.png"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	n.nodelabel,
	a.maintcontract,
	a.manufacturer,
	a.modelnumber,
	a.operatingsystem,
	a.serialnumber,
	to_date(a.dateinstalled,'DD.MM.YYYY') as dateinstalled,
	a.vendorphone,
	to_date(a.maintcontractexpires,'DD.MM.YYYY') AS maintcontractexpires,
	to_date(a.maintcontractexpires,'DD.MM.YYYY') - now() AS maintcontractleft,
	CASE
		WHEN
			to_date(a.maintcontractexpires,'DD.MM.YYYY') - now() < INTERVAL '0 days'
 			THEN 'WARNING'
		ELSE
			'NORMAL'
	END AS maintcontractstatus
FROM
	public.assets a
JOIN
	public.node n
ON
	a.nodeid = n.nodeid
WHERE
	n.nodetype = 'A' AND
	a.maintcontract IS NOT NULL  AND
	a.maintcontractexpires IS NOT NULL  AND
	a.maintcontractexpires != '' AND
	a.maintcontractexpires != 'n.v.' AND
	a.maintcontract != 'n.v.' AND
	a.maintcontract != '' AND
	to_date(a.maintcontractexpires,'DD.MM.YYYY') - now() < INTERVAL '$P!{WARNING_THRESHOLD_DAYS} days'
ORDER BY
	a.manufacturer ASC,
	a.maintcontractexpires DESC]]>
	</queryString>
